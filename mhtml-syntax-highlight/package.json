{
    "name": "mhtml-syntax",
    "displayName": "MHTML Syntax Highlighting",
    "description": "Syntax highlighting for the MHTML language.",
    "version": "0.0.1",
    "publisher": "Chirag Diwan",
    "engines": {
        "vscode": "^1.0.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "mhtml",
                "aliases": ["MHTML", "mhtml"],
                "extensions": [".mhtml"]
            }
        ],
        "grammars": [
            {
                "language": "mhtml",
                "scopeName": "source.mhtml",
                "path": "./MHTML.tmLanguage.json"
            }
        ]
    }
}

